   "source": [
    "# Create new DF with only 2019 season data\n",
    "\n",
    "stats_2019 = player_stats.loc[(player_stats[\"SEASON_ID\"] == \"2019-20\")]\n",
    "\n",
    "#Check the new DF.\n",
    "stats_2019.head()"
   ]
